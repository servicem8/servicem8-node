# servicem8 SDK Environment Variables
# Copy this file to .env and fill in your actual values
# DO NOT commit the .env file to version control

# Security Configuration
# The SDK supports loading security credentials from environment variables
# with the prefix: SERVICEM8_
#
# Security environment variables:
SERVICEM8_API_KEY=your_apikey_here
SERVICEM8_OAUTH2=your_oauth2_here

# Debug Configuration
# Enable HTTP request/response logging for debugging
SERVICEM8_DEBUG=false
